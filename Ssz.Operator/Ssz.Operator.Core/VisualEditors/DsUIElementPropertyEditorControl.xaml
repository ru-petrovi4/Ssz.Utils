<UserControl x:Class="Ssz.Operator.Core.VisualEditors.DsUIElementPropertyEditorControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid Name="MainGrid" Margin="5">
        <Grid.RowDefinitions>
            <RowDefinition Height="auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="auto" />
        </Grid.RowDefinitions>
        <ComboBox Grid.Row="0" Name="MainComboBox" Margin="0,0,0,0" IsReadOnly="True" />
        <TextBox Grid.Row="1" Name="MainTextBox" Margin="0,5,0,0" TextWrapping="Wrap" AcceptsReturn="True" />
    </Grid>
</UserControl>