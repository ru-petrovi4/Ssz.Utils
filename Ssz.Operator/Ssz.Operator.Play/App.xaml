<Application x:Class="Ssz.Operator.Play.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Startup="OnStartup" 
             Exit="OnExit"
             DispatcherUnhandledException="OnDispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary
                    Source="pack://application:,,,/Ssz.Operator.Core;component/Resources/CommonStyles.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Ssz.Operator.Core;component/Resources/DesignDrawing.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Ssz.Operator.Core;component/Resources/DesignDsShape.xaml" />                
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>