<UserControl x:Class="Ssz.Operator.Core.VisualEditors.PropertyGridTypeEditors.CloneableObjectTypeEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:res="clr-namespace:Ssz.Operator.Core.Properties"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary
                    Source="/Ssz.Operator.Core;component/VisualEditors/PropertyGridTypeEditors/EditorButtonStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Button Name="MainButton"
            IsTabStop="True"
            MinHeight="22"
            Content="{x:Static res:Resources.CloneableObjectProperties}"
            Style="{StaticResource EditorButtonStyle}"
            Click="ButtonClick" />
</UserControl>